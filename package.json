{
  "name": "YapayZeka",
  "version": "0.7.1",
  "private": true,
  "productName": "YapayZeka",
  "main": "electron/main.js",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@tensorflow/tfjs": "^1.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-jss": "^8.6.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.0.1",
    "react-spring": "^8.0.18"
  },
  "scripts": {
    "start": "export BROWSER=none && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build-electron": "mkdir build/src && cp -r electron/. build/electron && cp -r src/shared/. build/src/shared",
    "package": "electron-builder build --mac --win --x64 -c.extraMetadata.main=build/electron/main.js --publish never",
    "package-mac": "electron-builder build --mac -c.extraMetadata.main=build/electron/main.js --publish never",
    "package-win": "electron-builder build --win --x64 -c.extraMetadata.main=build/electron/main.js --publish never",
    "prepare-win": "react-scripts build && mkdir build/src && cp -r electron/. build/electron && cp -r src/shared/. build/src/shared && electron-builder build --win --x64 -c.extraMetadata.main=build/electron/main.js --publish never"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "electron-cra-example",
      "owner": "tercanfurkan"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "package-win": {
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^8.2.5",
    "electron-builder": "^22.5.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "gh-pages": "^2.0.1",
    "jest-canvas-mock": "^2.0.0-beta.1",
    "react-test-renderer": "^16.8.4"
  }
}
